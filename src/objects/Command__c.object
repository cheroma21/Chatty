    <validationRules>
        <fullName>Set_Status_Needs_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Actions__c, &apos;Value&apos;),  ISNULL(Action_Value__c ))</errorConditionFormula>
        <errorMessage>Must provide a value when setting the status for a non-boolean Thing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Status_Formula</fullName>
        <active>true</active>
        <description>Checks that a text formula includes a boolean value for evaluation</description>
        <errorConditionFormula>IF(NOT(ISBLANK( Status_Formula__c)),
NOT(
OR(
BEGINS( Status_Formula__c, &quot;&gt;&quot;),
BEGINS(Status_Formula__c, &quot;&lt;&quot;),
BEGINS(Status_Formula__c, &quot;!=&quot;),
BEGINS(Status_Formula__c, &quot;=&quot;)
))
, false)</errorConditionFormula>
        <errorDisplayField>Status_Formula__c</errorDisplayField>
        <errorMessage>A status formula must begin with one of the following operators:
&gt;, &lt;, =, !=</errorMessage>
    </validationRules>
